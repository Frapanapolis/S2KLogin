<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" 
xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
		 BackgroundColor="#63B1C1"
x:Class="S2KLogin.QNA"
		Title="QNA Page"
		 >
	 
	<ContentPage.Content>
		
 		<Grid>	
			 
			<Grid Padding="15,30,15,0">
			</Grid>
			<StackLayout Padding="35,80,35,25"  >

				 
 
         <Grid>
         	<Grid.RowDefinitions>
         		<RowDefinition Height="10*" />
		 	</Grid.RowDefinitions>
			<Grid.ColumnDefinitions>
    			<ColumnDefinition Width="1" />
				<ColumnDefinition Width="*" />
				<ColumnDefinition Width="1" />
			</Grid.ColumnDefinitions>
					<Button Text="Send" BackgroundColor="#333333" Grid.Row="1" Grid.Column="3" HeightRequest="15" WidthRequest="80" HorizontalOptions="CenterAndExpand" TextColor="White"  />
					<Entry x:Name="PWEntry"  Grid.Row="1" Grid.ColumnSpan="3" WidthRequest="50"  BackgroundColor="White"  ></Entry>
		</Grid>
			</StackLayout>
		</Grid>
	</ContentPage.Content>
</ContentPage>
